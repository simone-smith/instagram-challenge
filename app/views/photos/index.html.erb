<p id="notice"><%= notice %></p>

<div class="posts-wrapper row">
  <% @photos.each do |photo| %>
    <div class="post">
      <div class="post-head">
        <div class="name">
          Simone
        </div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag photo.image, class: 'img-responsive'), photo_path(photo) %>
      </div>
      <p class="caption">
        <%= photo.caption %>
      </p>
    </div>
  <% end %>
</div>
